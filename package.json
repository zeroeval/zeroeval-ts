{
  "name": "zeroeval",
  "version": "0.1.7",
  "description": "ZeroEval TypeScript SDK",
  "keywords": [
    "observability",
    "tracing",
    "monitoring",
    "langchain",
    "openai",
    "ai",
    "sdk",
    "analytics"
  ],
  "homepage": "https://zeroeval.com",
  "bugs": {
    "url": "https://github.com/zeroeval/zeroeval-ts/issues"
  },
  "license": "Apache-2.0",
  "author": [
    {
      "name": "Sebastian Crossa",
      "email": "seb@zeroeval.com",
      "url": "https://github.com/sebastiancrossa"
    },
    {
      "name": "Jonathan Chavez",
      "email": "jona@zeroeval.com",
      "url": "https://github.com/JonathanChavezTamales"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeroeval/zeroeval-ts.git",
    "directory": "zeroeval-ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "langchain": [
        "./dist/langchain.d.ts"
      ],
      "*": [
        "./dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./langchain": {
      "import": "./dist/langchain.js",
      "require": "./dist/langchain.cjs",
      "types": "./dist/langchain.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts src/langchain.ts --format esm,cjs --dts",
    "watch": "tsup src/index.ts src/langchain.ts --format esm,cjs --dts --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:core": "vitest tests/core",
    "test:perf": "vitest tests/performance",
    "test:multi": "node scripts/test-multi-version.js",
    "test:multi:core": "node scripts/test-multi-version.js core",
    "test:multi:perf": "node scripts/test-multi-version.js perf",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "example:basic": "npm run build && npx ts-node --esm examples/01-basic.ts",
    "example:basic-no-decorators": "npm run build && npx ts-node --esm examples/02-basic-no-decorators.ts",
    "example:openai": "npm run build && npx ts-node --esm examples/03-openai.ts",
    "example:langchain": "npm run build && npx ts-node --esm examples/04-langchain.ts",
    "example:vercel-ai": "npm run build && npx ts-node --esm examples/05-vercel-ai.ts",
    "example:signals": "npm run build && npx ts-node --esm examples/06-signals.ts",
    "prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "peerDependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@langchain/core": "^0.3.62",
    "@langchain/langgraph": "^0.3.6",
    "ai": "^4.3.16",
    "langchain": "^0.3.29",
    "openai": "^5.8.2"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/openai": {
      "optional": true
    },
    "@langchain/core": {
      "optional": true
    },
    "@langchain/langgraph": {
      "optional": true
    },
    "ai": {
      "optional": true
    },
    "langchain": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^20.19.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^7.2.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  }
}
