{
  "name": "@zeroeval/sdk",
  "version": "0.0.1",
  "description": "ZeroEval TypeScript SDK â€“ observability and tracing utilities",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./langchain": {
      "import": "./dist/langchain.js",
      "require": "./dist/langchain.cjs",
      "types": "./dist/langchain.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts src/langchain.ts --format esm,cjs --dts",
    "watch": "tsup src/index.ts src/langchain.ts --format esm,cjs --dts --watch",
    "example:basic": "npm run build && npx ts-node --esm examples/01-basic.ts",
    "example:basic-no-decorators": "npm run build && npx ts-node --esm examples/01-basic-no-decorators.ts",
    "example:openai": "npm run build && npx ts-node --esm examples/02-openai-wrapper.ts",
    "example:langchain": "npm run build && npx ts-node --esm examples/03-langchain-integration.ts"
  },
  "peerDependencies": {
    "@langchain/core": "^0.3.2",
    "langchain": "^0.3.2",
    "openai": "^4"
  },
  "peerDependenciesMeta": {
    "@langchain/core": {
      "optional": true
    },
    "@langchain/langgraph": {
      "optional": true
    },
    "langchain": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.19.4",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^7.2.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@langchain/langgraph": "^0.3.6"
  }
}
